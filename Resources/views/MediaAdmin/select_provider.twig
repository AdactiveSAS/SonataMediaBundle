{% block title %}{% trans 'title_select_provider' from 'MediaBundle' %}{% endblock %}

{% block content %}
    <ul>
        {% for code, provider in providers %}
            <li><a href="{{ url(urls.create.url, {'provider': code}) }}">{{ provider.name }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}